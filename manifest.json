{
    "name": "Etsy Email Scraper",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Download all emails from etsy order pages to csv list",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["tabs", "activeTab", "<all_urls>"]
  }